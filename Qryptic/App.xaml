<Application
    x:Class="Qryptic.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Qryptic"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/Fonts/Fonts.xaml" />
                <ResourceDictionary Source="Assets/Images/Icons.xaml" />
                <ResourceDictionary Source="Assets/Styles/MainStyles.xaml" />
                <ResourceDictionary Source="Assets/Styles/Colors.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
